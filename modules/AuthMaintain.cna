		sub ChangeAccount{
			local('$User $Pwd $groups $bid $Type $Do $Parameter $comment $tmp');
			$bid = $3['bid'];
			$User = $3['user'];
			$Pwd = $3['pass'];
			$groups = $3['localgroups'];
			$tmp = replace($3['comment'],' ','');
			$tmp = replace($tmp,'|','');
			
			$Type = $3['addtype'];   #API NET 
			$Do = $3['DoFt'];        #禁用 、启用、删除组用户
